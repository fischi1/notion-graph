{
  "name": "graph",
  "version": "1.0.0",
  "homepage": "/notion-graph",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "npm run lint && vite build",
    "preview": "vite preview --host 0.0.0.0",
    "lint": "eslint src && tsc",
    "deploy": "npm run build && npm version patch -m \"version %s\" && node ./deploy.js"
  },
  "devDependencies": {
    "rollup-plugin-visualizer": "^5.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "@fontsource/source-sans-pro": "^4.5.10",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "clsx": "^1.2.1",
    "d3": "^7.6.1",
    "dotenv": "^16.0.1",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^4.0.0",
    "jszip": "^3.10.0",
    "postcss-nested": "^5.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-remove-console": "^1.1.0"
  },
  "postcss": {
    "map": true,
    "plugins": {
      "postcss-nested": {}
    }
  }
}
